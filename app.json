{
  "name": "GDS-MD", 
  "description": "Super Bot WhatsApp",
  "repository": "https://github.com/PRINCE-GDS/GDS-MD",
  "logo": "https://i.imgur.com/izRryBm.jpg",
  "keywords": ["java", "bot", "whatsapp-bot", "whatsapp-automation"],

  "stack": "container",
  "env": {
    "SESSION_ID": {
      "description": "your session id",
      "required": true,
      "value": ""
      },
     "MODE": {
      "description": "mode public or private",
      "required": true,
      "value": "public"
      },
    "antidelete": {
      "description": "bot will forward deleted messages if you make it true",
      "required": false,
      "value": "false"
      },
     "HKEY": {
      "description": "Put your Heroku api key Here , Get one from here https://dashboard.heroku.com/account",
      "required": true,
      "value": ""
    },
     "HAPP": {
      "description": "Put the Heroku app name, same as above entered",
      "required": true,
      "value": ""
       },
     "OWNER_NAME": {
      "description": "Put owner name here.",
      "required": true,
      "value": "Prince♥️"
       },
     "OWNER_NUMBER": {
      "description": "Put owner number here.",
      "required": true,
      "value": "923042205427"
       },
     "BOT_NAME": {
      "description": "Put your name here for pack/author",
      "required": true,
      "value": "Prince♥️"
          },
    "PREFIX": {
      "description": "put any one symbol here except @ and + , leave it Blank if you want to use multiple prefix",
      "required": false,
      "value": ""
      },
     "REMOVEBG_KEY": {
      "description": "your RemoveBg api key",
      "required": false,
      "value": ""
    },
       "DATABASE_URL": {
      "description": "your mongodb database url",
      "required": false,
      "value": ""
       }      
  },
  
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "https://github.com/bogini/heroku-buildpack-graphicsmagick"
    }    
 ]
}
