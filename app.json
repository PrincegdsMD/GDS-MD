{
  "name": "GDS-MD", 
  "description": "Super Bot WhatsApp",
  "repository": "https://github.com/PRINCE-GDS/GDS-MD",
  "logo": "https://i.imgur.com/izRryBm.jpg",
  "keywords": ["java", "bot", "whatsapp-bot", "whatsapp-automation"],

  "stack": "container",
  "env": {
    "SESSION_ID": {
      "description": "put your session id here",
      "required": true,
      "value": ""
      },
     "MODE": {
      "description": "bot mode public or private",
      "required": true,
      "value": "public"
      },
    "antidelete": {
      "description": "bot will forward deleted messages if you make it true",
      "required": false,
      "value": "true"
      },
    "STATUSVIEW": {
      "description": "To auto view your contacts whatsapp status, for trun it off just delete this var",
      "required": false,
      "value": ""
      },
     "HKEY": {
      "description": "Put your Heroku api key Here.",
      "required": true,
      "value": ""
    },
     "HAPP": {
      "description": "Put the same above name here as a Heroku app name.",
      "required": true,
      "value": ""
       },
     "Video_Cap": {
      "description": "Video downloaded caption",
      "required": false,
      "value": "💗𝙃𝙀𝙍𝙀 𝙄𝙎 𝙔𝙊𝙐𝙍 𝙍𝙀𝙌𝙐𝙀𝙎𝙏𝙀𝘿 𝙑𝙄𝘿𝙀𝙊💗"
       },
     "BOT_NAME": {
      "description": "Put your name here for pack/author",
      "required": true,
      "value": "Prince♥️"
          },
    "PREFIX": {
      "description": "put any one symbol here for bot perfix",
      "required": false,
      "value": "."
      },
     "REMOVEBG_KEY": {
      "description": "your RemoveBg api key",
      "required": false,
      "value": ""
    },
       "DATABASE_URL": {
      "description": "your mongodb database url",
      "required": false,
      "value": ""
       }      
  },
  
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "https://github.com/bogini/heroku-buildpack-graphicsmagick"
    }    
 ]
}
